public class Packet {
    private long seqNum;
    private String protocol;
    private int port;

    public Packet(long seqNum, String protocol, int port) {
        this.seqNum = seqNum;
        this.protocol = protocol;
        this.port = port;
    }

    public long getSeqNum() {
        return seqNum;
    }

    public String getProtocol() {
        return protocol;
    }

    public int getPort() {
        return port;
    }
}
